{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/health": {
            "get": {
                "description": "Endpoint to perform a health check and verify the service status",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "health"
                ],
                "summary": "Performs a health check",
                "responses": {
                    "200": {
                        "description": "Health check result",
                        "schema": {
                            "$ref": "#/definitions/usecases.HealthStatus"
                        }
                    }
                }
            }
        },
        "/jobs/execute": {
            "post": {
                "description": "Executes the specified video jobs, such as trimming or concatenation, based on job IDs",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "job execute"
                ],
                "summary": "Executes specified video jobs",
                "parameters": [
                    {
                        "description": "List of job IDs to execute",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/request.ExecuteJobsRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "List of executed job IDs",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/response.JobIDResponse"
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request body",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/videos": {
            "get": {
                "description": "Fetches a list of all video details including trimming, concatenation jobs, and final video information",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "video"
                ],
                "summary": "Retrieves details of all videos",
                "responses": {
                    "200": {
                        "description": "List of video details",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/response.VideoDetailResponse"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            },
            "post": {
                "description": "Handles the upload of multiple video files, saves them, and returns their details",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "video"
                ],
                "summary": "Uploads video files",
                "parameters": [
                    {
                        "type": "file",
                        "description": "Video files to upload",
                        "name": "files",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Uploaded video details",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/response.VideoResponse"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/videos/concat": {
            "post": {
                "description": "Concatenates the specified videos into a single video",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "video"
                ],
                "summary": "Concatenates multiple videos",
                "parameters": [
                    {
                        "description": "List of video IDs to concatenate",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/request.ConcatVideosRequest"
                        }
                    }
                ],
                "responses": {
                    "202": {
                        "description": "Job ID of the concantnation task",
                        "schema": {
                            "$ref": "#/definitions/response.JobIDResponse"
                        }
                    },
                    "400": {
                        "description": "Bad request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/videos/{fid}/download": {
            "get": {
                "description": "Retrieves the download link for the final video based on the video ID",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "video"
                ],
                "summary": "Downloads the final video",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Final Video ID",
                        "name": "fid",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Download link for the final video",
                        "schema": {
                            "$ref": "#/definitions/response.VideoDownloadResponse"
                        }
                    },
                    "400": {
                        "description": "Invalid video ID",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/videos/{id}/trim": {
            "post": {
                "description": "Trims the specified video to the given start and end times",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "video"
                ],
                "summary": "Trims a video",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Video ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Trim start and end times",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/request.TrimVideoRequest"
                        }
                    }
                ],
                "responses": {
                    "202": {
                        "description": "Job ID of the trimming task",
                        "schema": {
                            "$ref": "#/definitions/response.JobIDResponse"
                        }
                    },
                    "400": {
                        "description": "Invalid video ID or bad request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "request.ConcatVideosRequest": {
            "type": "object",
            "required": [
                "videoIds"
            ],
            "properties": {
                "videoIds": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                }
            }
        },
        "request.ExecuteJobsRequest": {
            "type": "object",
            "properties": {
                "jobIds": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                }
            }
        },
        "request.TrimVideoRequest": {
            "type": "object",
            "required": [
                "trimEnd",
                "trimStart"
            ],
            "properties": {
                "trimEnd": {
                    "type": "string"
                },
                "trimStart": {
                    "type": "string"
                }
            }
        },
        "response.FinalVideoDetail": {
            "type": "object",
            "properties": {
                "downloadLink": {
                    "type": "string"
                },
                "filePath": {
                    "type": "string"
                },
                "filename": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                }
            }
        },
        "response.JobIDResponse": {
            "type": "object",
            "properties": {
                "jobID": {
                    "type": "integer"
                }
            }
        },
        "response.VideoDetailResponse": {
            "type": "object",
            "properties": {
                "concatJobs": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/response.VideoJobDetail"
                    }
                },
                "createdAt": {
                    "type": "string"
                },
                "duration": {
                    "type": "integer"
                },
                "filePath": {
                    "type": "string"
                },
                "filename": {
                    "type": "string"
                },
                "finalVideoDetail": {
                    "$ref": "#/definitions/response.FinalVideoDetail"
                },
                "id": {
                    "type": "integer"
                },
                "status": {
                    "type": "string"
                },
                "trimJobs": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/response.VideoJobDetail"
                    }
                },
                "updatedAt": {
                    "type": "string"
                }
            }
        },
        "response.VideoDownloadResponse": {
            "type": "object",
            "properties": {
                "downloadLink": {
                    "type": "string"
                },
                "filePath": {
                    "type": "string"
                },
                "filename": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                }
            }
        },
        "response.VideoJobDetail": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "jobType": {
                    "type": "string"
                },
                "parameters": {
                    "type": "string"
                },
                "resultFilePath": {
                    "type": "string"
                },
                "status": {
                    "type": "string"
                }
            }
        },
        "response.VideoResponse": {
            "type": "object",
            "properties": {
                "createdAt": {
                    "type": "string"
                },
                "duration": {
                    "type": "integer"
                },
                "filePath": {
                    "type": "string"
                },
                "filename": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "status": {
                    "type": "string"
                },
                "updatedAt": {
                    "type": "string"
                }
            }
        },
        "usecases.HealthStatus": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string"
                },
                "status": {
                    "type": "string"
                }
            }
        }
    }
}